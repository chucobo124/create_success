<form class="form-wrapper">
  <input type="text" id="search" placeholder="Search for..." required>
  <input type="submit" value="go" id="submit">
</form>
<%= link_to t('faq_category.create'), new_admin_faq_category_path, class: 'btn info'%>

<table>
  <tr>
    <th><%= t('faq_category.title')%></th>
    <th><%= t('faq_category.description')%></th>
    <th><%= t('aciton.label')%></th>
  </tr>
  <% @admin_faq_category.each do |faq_category|%>
    <tr>
      <th><%= link_to t(faq_category.title), admin_faq_category_faq_index_path(faq_category.id)%></th>
      <th><%= faq_category.description.html_safe%></th>
      <th>
        <%= link_to t('action.update'), admin_faq_category_path(faq_category.id), class: 'btn success'%>
        <%= link_to t('action.delete'), admin_faq_category_path(faq_category.id), method: 'delete', class: 'btn danger'%>
      </th>
    </tr>
  <%end%>
</table>
